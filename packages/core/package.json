    "@material-did/qr": "0.1.0",
    "@material-did/sidetree": "0.1.0",
    "@material-did/universal-wallet": "0.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@transmute/universal-wallet": "0.0.6"
  }
